{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend server",
  "main": "index.js",
  "scripts": {
    "init": "npm install",
    "backend": "node index.js --port 3000",
    "backend-prod": "node index.js",
    "bg:start": "forever start --uid bundlechecker ./index.js",
